[user]
	name = Hussein Al Abry
	email = hussein@zidhuss.tech
[init]
	defaultBranch = main
[commit]
	verbose = true
[merge]
	conflictstyle = diff3
[core]
	excludesfile = /Users/abry/.gitignore
    pager = delta
[interactive]
    diffFilter = delta --color-only
[fetch]
	prune = true
[pull]
	rebase = true
[rebase]
	autoStash = true
[delta]
    navigate = true # use n and N to move between diff sections
    light = true    # set to true if you're in a terminal w/ a light background color (e.g. the default macOS terminal)
